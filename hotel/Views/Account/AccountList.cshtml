@model IEnumerable<zlobek.Entities.Account>

 

 
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>ACCOUNT LIST</title>
    <link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.min.css?h=56ac0ec133e27aff839b2e0be71ab12f">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/assets/css/styles.min.css?h=f8351878772c8f1418e76fda12aa67fb">
</head>
<body id="page-top" data-bs-spy="scroll" data-bs-target="#mainNav" data-bs-offset="77" style="height: 640px;color: var(--bs-code-color);">
    <header class="masthead" style="background-image: url('/assets/img/intro-bg.jpg?h=145cae4a8124ee5dac950230c1552d3c');">
        <div class="intro-body" style="background: #ffffff;height: 790.188px;width: 1063px;margin-right: 0px;">
            <nav class="navbar navbar-light navbar-expand-md fixed-top border-primary text-center border rounded-0 border-1" id="mainNav" style="background: #FFF0F5;padding: 0px 0px;margin-left: 0px;--bs-primary: #ed2893;--bs-primary-rgb: 237,40,147;color: #FFF0F5;">
                <div class="container"><a class="navbar-brand text-center" href="#" style="color: var(--bs-pink);font-size: 30px;font-family: 'Abril Fatface', serif;text-align: center;margin-right: 397px;padding-right: 8px;padding-left: 0px;margin-left: 314px;">NIEPUBLICZNY ŻŁOBEK MYSIE-PYSIE</a>
                    <a class="navbar-brand text-center" href="#" style="color: var(--bs-pink);font-size: 30px;font-family: 'Abril Fatface', serif;text-align: center;margin-right: 397px;padding-right: 8px;padding-left: 0px;margin-left: 346px;">NIEPUBLICZNY ŻŁOBEK MYSIE-PYSIE</a>
                    <button class="navbar-toggler navbar-toggler-right" data-bs-toggle="collapse" data-bs-target="#navbarResponsive-1" type="button" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation" value="Menu"><i class="fa fa-bars"></i>
                    </button>
                 </div>
            </nav>
        <section style="color: var(--bs-pink);height: 500px;background: linear-gradient(-175deg, #f2afc1 -90%, white), #f4bfd1;margin-top: -172px;border-right: 1px solid rgba(214,51,132,0.64);">
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th style="vertical-align: middle;width: 50px;color: var(--bs-code-color);text-align: center;font-family: Lora, serif;font-size: 13px;border: 1px solid var(--bs-pink);border-top-width: 0px;border-right: 1px solid rgba(237,114,170,0.6);border-bottom: 1px solid rgba(237,114,170,0.6);">&nbsp;ACCOUNT ID</th>
                            <th style="vertical-align: middle;width: 50px;border-right: 1px solid rgba(237,114,170,0.6);border-bottom-color: rgba(237,114,170,0.6);color: var(--bs-code-color);text-align: center;font-size: 13px;">NAME</th>
                            <th style="vertical-align: middle;width: 50px;color: var(--bs-code-color);border-right: 1px solid rgba(237,114,170,0.6);border-bottom-color: rgba(237,114,170,0.6);font-size: 13px;">SURNAME</th>
                            <th style="vertical-align: middle;width: 50px;color: var(--bs-code-color);border-right: 1px solid rgba(237,114,170,0.6);border-bottom-color: rgba(237,114,170,0.6);font-size: 13px;">EMAIL</th>
                            <th style="vertical-align: middle;width: 50px;color: var(--bs-code-color);border-right: 1px solid rgba(237,114,170,0.6);border-bottom-color: rgba(237,114,170,0.6);font-size: 13px;">PHONE NUMBER</th>
                            <th style="vertical-align: middle;width: 50px;color: var(--bs-code-color);font-size: 13px;border-right-width: 1px;border-right-color: rgba(237,114,170,0.6);border-bottom-width: 1px;border-bottom-color: rgba(237,114,170,0.6);">ROLE ID</th>
        
                           </th></tr></thead>
                            <tbody>
                            
        @foreach (var account in Model)
            
                                {
                            <tr>
                                <td style="border: 1px solid rgba(237,114,170,0.6);">@account.AccountId</td>
                                    <td style="border: 1px solid rgba(237,114,170,0.6);">@account.Name</td>
                                    <td style="border: 1px solid rgba(237,114,170,0.6);">@account.Surname</td>
                                    <td style="border: 1px solid rgba(237,114,170,0.6);">@account.Email</td>
                                    <td style="border: 1px solid rgba(237,114,170,0.6);">@account.PhoneNumber</td>
                                    <td style="border: 1px solid rgba(237,114,170,0.6);">@account.RoleId</td>
                                </tr>
                               } 
                                </tbody>
                                </table>
                                </div>
                                </section>
                                </div>
                                </header>
    <footer class="border rounded-0 border-primary" style="background: #FFF0F5;height: 89.398px;margin-top: -28px;--bs-primary: #ed2893;--bs-primary-rgb: 237,40,147;"
    ><div class="container text-center"><p style="margin-top: -12px;">Copyright © Niepubliczny Żłobek Mysie-Pysie w Krakowie | Projekt wykonała Jowita Jońca 13680</p>
        </div></footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/assets/js/script.min.js?h=abade7a6b58120c54533d461e48e360b"></script>
</body>
